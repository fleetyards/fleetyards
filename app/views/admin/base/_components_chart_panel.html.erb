<div class="panel-wrapper panel-outer-spacing">
  <div class="panel">
    <div class="panel-inner">
      <div class="panel-heading">
        <h2 class="panel-title">
          <%= I18n.t(:"labels.panels.components_by_class") %>
        </h2>
      </div>
      <div class="panel-body">
        <div
          data-controller="chart"
          data-chart-url-value="<%= api_url(components_by_class_api_v1_stats_path) %>"
          data-chart-type-value="pie"
          data-chart-height-value="453px"
          data-chart-tooltip-value="component-pie"
        >
        </div>
      </div>
    </div>
  </div>
</div>
