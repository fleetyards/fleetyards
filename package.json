{
  "name": "fleetyards",
  "private": true,
  "version": "5.27.21",
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "cypress run --browser chrome --headless",
    "test:e2e:report": "generate-mochawesome-report --jsonDir test/reports/e2e --output test/reports/e2e/html --charts",
    "lint": "yarn run lint:js && yarn run lint:prettier && yarn run lint:ts",
    "lint:fix": "yarn run lint:js && yarn run lint:prettier:fix",
    "lint:js": "eslint --ext .js,.ts,.vue --fix app/frontend",
    "lint:ts": "yarn run vue-tsc && yarn run tsc",
    "lint:prettier": "prettier app/frontend --check",
    "lint:prettier:fix": "prettier app/frontend --write",
    "vue-tsc": "vue-tsc --noEmit",
    "tsc": "tsc --noEmit --pretty",
    "postinstall": "yarn generate-api-client && yarn generate-admin-api-client",
    "generate-api-client": "openapi --input ./swagger/v1/schema.yaml --output ./app/frontend/services/fyApi --useOptions --client axios --request app/frontend/services/request.template.ts --name FyApi",
    "generate-admin-api-client": "openapi --input ./swagger/admin/v1/schema.yaml --output ./app/frontend/services/fyAdminApi --useOptions --client axios --request app/frontend/services/request.template.ts --name FyAdminApi",
    "validate-schema": "npx @redocly/cli@latest lint ./swagger/v1/schema.yaml",
    "validate-admin-schema": "npx @redocly/cli@latest lint ./swagger/admin/v1/schema.yaml"
  },
