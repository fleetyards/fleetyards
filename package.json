{
  "name": "fleetyards",
  "private": true,
  "version": "5.31.63",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "dev:vite": "bin/vite dev --mode=test",
    "dev:playwright:ui": "DEBUG=pw:webserver playwright test --ui",
    "test:e2e:startserver": "RAILS_ENV=test bundle exec foreman run --env .env.test rails s --pid tmp/pids/server_e2e.pid",
    "test:e2e:open": "concurrently \"pnpm dev:vite\" \"pnpm dev:playwright:ui\" --kill-others",
    "test:e2e:run": "playwright test",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install --with-deps",
    "lint": "pnpm run lint:js && pnpm run lint:prettier && pnpm run lint:ts",
    "lint:fix": "pnpm run lint:js && pnpm run lint:prettier:fix",
    "lint:js": "eslint --ext .js,.ts,.vue --fix app/frontend",
    "lint:ts": "pnpm run vue-tsc && pnpm run tsc",
    "lint:prettier": "prettier app/frontend --check",
    "lint:prettier:fix": "prettier app/frontend --write",
    "vue-tsc": "vue-tsc --noEmit",
    "tsc": "tsc --noEmit --pretty",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm generate-api-client",
    "generate-api-client": "orval -c orval.config.ts",
    "validate-schema": "npx @redocly/cli@latest lint ./swagger/v1/schema.yaml",
    "validate-admin-schema": "npx @redocly/cli@latest lint ./swagger/admin/v1/schema.yaml",
    "knip": "knip"
  },
