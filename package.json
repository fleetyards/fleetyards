    "docs:old": "aglio --theme-variables slate --theme-template triple -i docs/v1/main.apib -o public/docs/v1.html",
    "docs:watch": "aglio --theme-variables slate --theme-template triple -i docs/v1/main.apib --server",
    "test": "jest",
    "test:ci": "jest -w 1",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --notify",
    "test:e2e": "cypress run --browser chrome --headless",
    "test:e2e:report": "generate-mochawesome-report --jsonDir test/reports/e2e --output test/reports/e2e/html --charts",
    "lint": "yarn run lint:js && yarn run lint:style",
    "lint:js": "eslint --ext .js,.ts,.vue --fix app/packs test/javascript",
    "lint:style": "stylelint \"app/packs/**/*.scss\" \"app/packs/**/*.vue\" \"app/assets/**/*.scss\" --fix",
    "webpack:analyze": "mkdir -p public/packs && node_modules/.bin/webpack --config config/webpack/production.js --profile --json > public/packs/stats.json && node_modules/.bin/webpack-bundle-analyzer public/packs/stats.json"
  },
