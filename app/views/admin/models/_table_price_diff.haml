.table-responsive
  %table.table.table-hover.table-striped
    %thead
      %tr
        %th.rsi-id=sort_link(@q, :rsi_id, 'RSI ID')
        %th=sort_link(@q, :pledge_price)
        %th=sort_link(@q, :last_pledge_price)
        %th.actions.actions-four-ladda=I18n.t(:"labels.table.actions")
    %tbody
      = render partial: "admin/models/show_price_diff", collection: @models, as: :model

      - if @models.blank?
        = render "shared/tables/blank", count: 4, resource: "model"
